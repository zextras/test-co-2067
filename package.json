{
	"name": "@zextras/carbonio-ui-commons",
	"version": "1.0.0",
	"description": "Carbonio UI Commons",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./*": null
	},
	"engines": {
		"node": "v20",
		"npm": "v10"
	},
	"scripts": {
		"build": "npm run build:pkg && npm run build:lib",
		"build:dev": "npm run build:pkg -- --dev --pkgRel $(date +%s) && npm run build:lib",
		"build:pkg": "sdk build",
		"prebuild:lib": "rm -rf lib",
		"build:lib": "tsc -p tsconfig.lib.json",
		"deploy": "npm run build:pkg && sdk deploy",
		"deploy:dev": "npm run build:pkg -- -d && sdk deploy",
		"test": "TZ=Europe/Rome jest --testTimeout=20000 --maxWorkers=50%",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx --resolve-plugins-relative-to node_modules/@zextras/carbonio-ui-configs src",
		"lint-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx --quiet --resolve-plugins-relative-to node_modules/@zextras/carbonio-ui-configs",
		"lint-errors": "eslint --ext .js,.jsx,.ts,.tsx --quiet --resolve-plugins-relative-to node_modules/@zextras/carbonio-ui-configs src",
		"lint-stats": "eslint --ext .js,.jsx,.ts,.tsx --format node_modules/eslint-stats/byErrorAndWarningStacked --resolve-plugins-relative-to node_modules/@zextras/carbonio-ui-configs src",
		"lint-check": "npm run lint-errors; npm run type-check",
		"prestart": "npm run prebuild:pkg",
		"start": "sdk watch",
		"prepack": "npm run build:lib",
		"packTo": "npm pack --pack-destination $PKG_PATH && cd $PKG_PATH && npm i zextras-carbonio-ui-commons-$npm_package_version.tgz",
		"deploy-on-module": "npm pack && rm -rf $PKG_PATH/node_modules/@zextras/carbonio-ui-commons/* && tar -xf zextras-carbonio-ui-commons-$npm_package_version.tgz -C $PKG_PATH/node_modules/@zextras/carbonio-ui-commons/ --strip-components 1"
	},
	"keywords": [],
	"author": "Zextras <https://www.zextras.com/carbonio/>",
	"maintainers": [
		"Gabriele Marino <gabriele.marino@zextras.com>",
		"Giuliano Caregnato <giuliano.caregnato@zextras.com>",
		"Dhaval Dodiya <dhaval.dodiya@zextras.com>",
		"Francesco Gottardi <francesco.gottardi@zextras.com>",
		"Luca Stauble <luca.stauble@zextras.com>"
	],
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"@tsconfig/node20": "^20.1.5",
		"@babel/core": "^7.24.0",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-runtime": "^7.24.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.21.0",
		"@faker-js/faker": "^8.4.1",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/lodash": "^4.17.4",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.1",
		"@types/react-redux": "^7.1.25",
		"react-redux": "^8.0.5",
		"@types/styled-components": "^5.1.26",
		"@types/uuid": "^10.0.0",
		"@zextras/carbonio-search-ui": "github:zextras/carbonio-search-ui#devel",
		"@zextras/carbonio-ui-configs": "^2.1.0",
		"@zextras/carbonio-ui-sdk": "^2.1.0",
		"babel-plugin-styled-components": "^2.0.7",
		"babel-plugin-transform-import-meta": "^2.2.0",
		"eslint-plugin-jest-dom": "^5.4.0",
		"eslint-plugin-notice": "^0.9.10",
		"eslint-plugin-testing-library": "^5.10.1",
		"eslint-plugin-unused-imports": "^2.0.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-fail-on-console": "^3.1.2",
		"jest-fetch-mock": "^3.0.3",
		"msw": "^2.2.3",
		"npm": "^10.5.0",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@emotion/react": "^11.11.4",
		"@emotion/styled": "^11.10.5",
		"@mui/icons-material": "^5.15.12",
		"@mui/material": "^5.15.12",
		"@zextras/carbonio-design-system": "^10.1.1",
		"@zextras/carbonio-shell-ui": "^11.0.1",
		"@zextras/carbonio-ui-preview": "^3.2.1",
		"core-js": "^3.36.0",
		"eslint": "^8.57.0",
		"i18next": "^22.4.13",
		"immer": "^9.0.19",
		"jest-junit": "^15.0.0",
		"lodash": "^4.17.21",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-i18next": "^12.2.0",
		"react-router-dom": "^6.30.0",
		"styled-components": "^6.1.13",
		"uuid": "^9.0.0",
		"zustand": "^4.5.2"
	},
	"files": [
		"lib",
		"README.md"
	],
	"browserslist": [
		">1%",
		"last 1 version",
		"Firefox ESR",
		"not dead"
	],
	"carbonio": {
		"name": "carbonio-ui-commons",
		"priority": 5,
		"type": "carbonio",
		"attrKey": "zimbraFeatureMailEnabled",
		"display": "Carbonio UI Commons"
	}
}
